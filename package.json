{
  "name": "watchit",
  "author": "ZorrillosDev",
  "version": "0.1.0-beta.5",
  "private": true,
  "main": "./src/main/index.js",
  "description": "Open movies everywhere",
  "email": "support@zorrillos.com",
  "homepage": "./",
  "license": "LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZorrillosDev/watchit-desktop.git"
  },
  "dependencies": {
    "dlnacasts2": "^0.2.0",
    "electron-log": "^4.3.2",
    "electron-updater": "^4.3.5",
    "fs-extra": "^8.1.0",
    "go-ipfs": "^0.6.0",
    "hls.js": "^0.14.17",
    "iconv-lite": "^0.5.1",
    "identity-obj-proxy": "3.0.0",
    "ip": "^1.1.5",
    "ipfs-http-client": "^47.0.1",
    "ipfsd-ctl": "^7.2.0",
    "it-all": "^1.0.5",
    "it-last": "^1.0.5",
    "it-reduce": "^1.0.5",
    "jschardet": "^2.1.1",
    "leveldown": "^5.6.0",
    "linvodb3": "^3.26.0",
    "mime": "^2.4.6",
    "msgpack-lite": "^0.1.26",
    "orbit-db": "^0.26.0",
    "parse-torrent": "^7.1.3",
    "peerflix": "^0.39.0",
    "plyr": "^3.6.2",
    "read-torrent": "^1.3.1",
    "rimraf": "^3.0.2",
    "secp256k1": "^4.0.2",
    "shortid": "^2.2.15",
    "srt2vtt": "^1.3.1",
    "streaming-iterables": "^5.0.4",
    "subtitle": "^2.0.3",
    "unzipper": "^0.10.11",
    "v8-compile-cache": "^2.2.0",
    "webtorrent-health": "^1.1.2"
  },
  "build": {
    "productName": "watchit",
    "appId": "org.zorrillos.WatchIt",
    "publish": [
      {
        "provider": "github",
        "owner": "ZorrillosDev",
        "repo": "watchit-desktop",
        "private": false
      }
    ],
    "files": [
      "src/build/**/*",
      "node_modules/**/*",
      "src/main/**/*",
      "src/render/media/icons/*",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "nsis": {
      "deleteAppDataOnUninstall": true
    },
    "linux": {
      "target": [
        "deb",
        "AppImage",
        "tar.gz"
      ],
      "category": "Video",
      "maintainer": "ZorrillosDev"
    },
    "directories": {
      "buildResources": "src/render/media/icons",
      "output": "release"
    }
  },
  "scripts": {
    "start": "BROWSER=none node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js  --watchAll=false -u",
    "electron": "cross-env LOG=debug IPFS_FD_MAX=10000 ENV=development $(npm bin)/electron .",
    "lint": "eslint --ext js,jsx",
    "lint:fix": "npm run lint -s -- --fix src ",
    "package": "npm run build && electron-builder build",
    "package:mac": "npm run package -- --mac",
    "package:linux": "npm run package -- --linux",
    "package:win": "npm run package -- --win --x64 --ia32",
    "publish:mac": "npm run package:mac -- --publish always",
    "publish:linux": "npm run package:linux -- --publish always",
    "publish:win": "npm run package:win -- --publish always",
    "preinstall": "bash -c \"npm install electron-log && node scripts/preinstall.js\"",
    "postinstall": "bash -c \"node scripts/postinstall.js && node-prune\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "peerDependencies": {
    "windows-elevate": "^1.0.1",
    "go-ipfs": "^0.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run lint:fix"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "/home/geolffrey/WebstormProjects/watchit-desktop/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^10.0.0",
    "electron-builder": "^22.10.5",
    "electron-reload": "^1.5.0",
    "husky": "^4.3.8",
    "moment-timezone": "^0.5.31",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dev-utils": ">=11.0.4",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-particles-js": "^3.4.1",
    "react-player": "^2.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-tooltip": "^4.2.6",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
    "semver": "6.3.0",
    "tsparticles": "^1.19.2",
    "webpack-merge": "^5.7.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
