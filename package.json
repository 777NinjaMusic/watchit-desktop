  },
  "build": {
    "productName": "watchit",
    "appId": "org.zorrillos.WatchIt",
    "publish": [
      {
        "provider": "github",
        "owner": "ZorrillosDev",
        "repo": "watchit-desktop",
        "private": false
      }
    ],
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "src/media/icons/*",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "nsis": {
      "deleteAppDataOnUninstall": true
    },
    "linux": {
      "target": [
        "deb",
        "AppImage",
        "tar.gz"
      ],
      "category": "Video",
      "maintainer": "ZorrillosDev"
    },
    "directories": {
      "buildResources": "src/media/icons",
      "output": "release"
    }
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js  --watchAll=false",
    "electron": "cross-env BROWSER=none LOG=debug IPFS_FD_MAX=10000 ENV=dev $(npm bin)/electron .",
    "lint": "eslint --ext js,jsx",
    "lint:fix": "npm run lint -s -- --fix src ",
    "package": "npm run build && electron-builder build",
    "package:mac": "npm run package -- --mac",
    "package:linux": "npm run package -- --linux",
    "package:win": "npm run package -- --win --x64 --ia32",
    "publish:mac": "npm run package:mac -- --publish always",
    "publish:linux": "npm run package:linux -- --publish always",
    "publish:win": "npm run package:win -- --publish always",
    "preinstall": "bash -c \"npm install electron-log && node scripts/preinstall.js\"",
    "postinstall": "node scripts/postinstall.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "peerDependencies": {
    "windows-elevate": "^1.0.1",
    "go-ipfs": "^0.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "/home/geolffrey/WebstormProjects/watchit-desktop/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "eslint": "^6.6.0",
    "husky": "^4.3.8",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty": "^2.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
